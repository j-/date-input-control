    "build": "tsc",
    "clean": "rimraf \"**.{js,d.ts}\"",
    "prepare": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-/date-input-control.git"
